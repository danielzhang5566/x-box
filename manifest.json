{
  "manifest_version": 2,
  "name": "X-BOX",
  "version": "2.0.0",
  "description": "X-Box 工具箱，可以计算加班打卡时间，可以自定义配置工作日、节假日。",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "X-BOX",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
