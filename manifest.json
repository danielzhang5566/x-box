{
  "manifest_version": 2,
  "name": "X-BOX",
  "version": "1.1.4",
  "description": "Chrome浏览器插件，集合一些小工具，完善ing~",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "default_title": "X-BOX",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
